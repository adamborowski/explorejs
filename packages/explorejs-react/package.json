{
  "name": "explorejs-react",
  "version": "0.0.0",
  "description": "Explorejs React bindings - configuration exposed as React components",
  "main": "lib/explorejs-react.js",
  "scripts": {
    "build": "webpack --env build --display-error-details",
    "build:dev": "webpack --env dev --display-error-details",
    "dev": "webpack --display-modules --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/**/*.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/**/*.js"
  },
  "devDependencies": {
    "babel-preset-react": "6.16.0",
    "dygraphs": "1.1.1",
    "explorejs-adapters": "^0.0.0",
    "explorejs-lib": "^0.0.0",
    "jquery": "3.2.1",
    "mocha": "2.3.4",
    "plotly.js": "^1.27.1",
    "prop-types": "^15.5.10",
    "react": "15.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamborowski/explorejs.git"
  },
  "keywords": [
    "explorejs",
    "react"
  ],
  "author": "Adam Borowski",
  "license": "ISC",
  "dependencies": {}
}
